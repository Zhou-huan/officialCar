.examine {
    padding: 130px 36px 120px;
    font-size: 28px;
    .icons {
        color: gray;
        width: 36px;
        height: 36px;
    }
    .title {
        color: #333;
        font-weight: bold;
    }
    .banner {
        width: 680px;
        height: 144px;
        margin: 0 auto;
    }
    .useCar {
        background: #FFFFFF;
        box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.04);
        border-radius: 12px;
        margin-top: 20px;
        padding: 35px;
        box-sizing: border-box;
        .useTitle {
            display: flex;
            flex-direction: row;
            justify-content: space-between;
            align-items: center;
            p {
                padding-top: 10px;
            }
            img {
                width: 30px;
                height: 30px;
            }
        }
        .secondUl {
            li {
                display: flex;
                flex-direction: row;
                justify-content: space-between;
                padding-top: 20px;
                img {
                    width: 40px;
                    height: 40px;
                }
            }
        }
    }
    .firstUl {
        display: flex;
        flex-direction: row;
        justify-content: space-around;
        padding: 40px 0 20px;
        border-bottom: 1px solid #eee;
        li {
            text-align: center;
        }
    }
    .record {
        background: #FFFFFF;
        box-shadow: 0px 4px 20px 0px rgba(0, 0, 0, 0.04);
        border-radius: 12px;
        margin-top: 28px;
        padding: 35px;
        box-sizing: border-box;
        .firstUl {
            border-bottom: none;
        }
    }
  }